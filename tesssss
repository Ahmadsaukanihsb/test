-- Instructions Section
--------------------------------------------------------------------------------
local instructionsFrame = Instance.new("Frame")
instructionsFrame.Name = "InstructionsFrame"
instructionsFrame.Size = UDim2.new(1, 0, 0, 110)
instructionsFrame.BackgroundTransparency = 1
instructionsFrame.Parent = mainFrame

local instructionsListLayout = Instance.new("UIListLayout")
instructionsListLayout.FillDirection = Enum.FillDirection.Vertical
instructionsListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Left -- Instruksi rata kiri
instructionsListLayout.Padding = UDim.new(0, 4)
instructionsListLayout.Parent = instructionsFrame

-- Icon Instruksi (kertas/daftar)
local instructionsIcon = Instance.new("ImageLabel")
instructionsIcon.Name = "InstructionsIcon"
instructionsIcon.Size = UDim2.new(0, 20, 0, 20)
instructionsIcon.BackgroundTransparency = 1
instructionsIcon.Image = "rbxassetid://5971485304" -- Ganti dengan ID ikon dokumen/instruksi yang sesuai
instructionsIcon.ImageColor3 = Color3.fromRGB(200, 200, 200) -- Warna ikon instruksi
instructionsIcon.Parent = instructionsFrame

-- Instruksi teks
local instructions = {
    "1. Hold a RED FOX/DRAGONFLY/RACCOON",
    "2. Keep inventory open",
    "3. Press DUPLICATE button",
    "4. Wait 5-10 Minutes"
}

for i, text in ipairs(instructions) do
    local instText = Instance.new("TextLabel")
    instText.Name = "InstructionsText" .. i
    instText.Size = UDim2.new(1, 0, 0, 16)
    instText.BackgroundTransparency = 1
    instText.Font = Enum.Font.SourceSansSemibold
    instText.TextColor3 = Color3.fromRGB(255, 255, 255)
    instText.TextSize = 13
    instText.TextXAlignment = Enum.TextXAlignment.Left
    instText.Text = text
    instText.Parent = instructionsFrame
end

