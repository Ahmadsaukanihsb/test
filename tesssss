--------------------------------------------------------------------------------
-- Sistem Sembunyikan/Tampilkan GUI (Alternatif Keyboard untuk PC)
--------------------------------------------------------------------------------
local toggleKey = Enum.KeyCode.Insert -- Tombol untuk menyembunyikan/menampilkan GUI (bisa diganti)

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    -- Pastikan input bukan dari game (misal: mengetik di chat)
    if not gameProcessedEvent and input.KeyCode == toggleKey then
        isGuiVisible = not isGuiVisible -- Balik status visibilitas
        screenGui.Enabled = isGuiVisible -- Terapkan status ke ScreenGui

        if isGuiVisible then
            toggleButton.ImageColor3 = Color3.fromRGB(150, 150, 150) -- Warna ikon mata saat terlihat
            print("GUI 'DAIZZ DUPER X' ditampilkan (via keyboard).")
        else
            toggleButton.ImageColor3 = Color3.fromRGB(80, 80, 80) -- Warna ikon mata saat tersembunyi
            print("GUI 'DAIZZ DUPER X' disembunyikan (via keyboard).")
        end
    end
end)

print("Tekan tombol 'mata' di GUI atau tombol '" .. toggleKey.Name .. "' untuk menyembunyikan/menampilkan GUI.")
print("Tekan tombol 'X' di GUI untuk menghapus GUI.")
