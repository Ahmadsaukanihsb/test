lightningIcon.Image = "rbxassetid://6034177579" -- Ganti dengan ID ikon petir yang sesuai (contoh: warning icon)
lightningIcon.ImageColor3 = Color3.fromRGB(255, 165, 0) -- Warna oranye
lightningIcon.Parent = headerFrame

-- Judul "DAIZZ DUPER X"
