--------------------------------------------------------------------------------
-- Try Again Button Container
--------------------------------------------------------------------------------
local buttonContainer = Instance.new("Frame")
buttonContainer.Name = "ButtonContainer"
buttonContainer.Size = UDim2.new(1, 0, 0, 40) -- Lebar penuh, tinggi sesuai tombol
buttonContainer.BackgroundTransparency = 1
buttonContainer.Parent = mainFrame

local buttonContainerLayout = Instance.new("UIListLayout")
buttonContainerLayout.FillDirection = Enum.FillDirection.Horizontal
buttonContainerLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center -- Pusatkan tombol secara horizontal
buttonContainerLayout.VerticalAlignment = Enum.VerticalAlignment.Center -- Pusatkan tombol secara vertikal
buttonContainerLayout.Parent = buttonContainer

-- Try Again Button
local tryAgainButton = Instance.new("TextButton")
tryAgainButton.Name = "TryAgainButton"
tryAgainButton.Size = UDim2.new(0, 240, 1, 0) -- Ukuran tombol relatif terhadap container (0.8 * 300 = 240px)
tryAgainButton.BackgroundColor3 = Color3.fromRGB(200, 30, 30) -- Warna merah tombol
tryAgainButton.BorderSizePixel = 0
tryAgainButton.Font = Enum.Font.SourceSansBold
tryAgainButton.TextColor3 = Color3.fromRGB(255, 255, 255)
tryAgainButton.TextSize = 20
tryAgainButton.Text = "TRY AGAIN"
tryAgainButton.Parent = buttonContainer -- Parent ke kontainer baru

-- Tambahkan UICorner ke tombol
local buttonCorner = Instance.new("UICorner")
buttonCorner.CornerRadius = UDim.new(0, 8)
buttonCorner.Parent = tryAgainButton

-- Tambahkan UIGradient ke tombol untuk efek visual
local buttonGradient = Instance.new("UIGradient")
buttonGradient.Color = ColorSequence.new(Color3.fromRGB(220, 50, 50), Color3.fromRGB(180, 20, 20))
buttonGradient.Rotation = 90
buttonGradient.Parent = tryAgainButton

-- Logika ketika tombol 'TRY AGAIN' diklik
tryAgainButton.MouseButton1Click:Connect(function()
    print("Tombol 'TRY AGAIN' diklik!")

    local requiredPets = {"Red Fox", "Dragonfly", "Raccoon", "Deer"}
    local hasRequiredPet = false

    -- Cek di Backpack (inventori)
    for _, item in ipairs(LocalPlayer.Backpack:GetChildren()) do
        if table.find(requiredPets, item.Name) then
            hasRequiredPet = true
            break
        end
    end

    -- Cek di Character (jika sedang dipegang)
    if not hasRequiredPet and LocalPlayer.Character then
        for _, item in ipairs(LocalPlayer.Character:GetChildren()) do
            if item:IsA("Tool") and table.find(requiredPets, item.Name) then
                hasRequiredPet = true
                break
            end
        end
    end

    if hasRequiredPet then
        print("Pet yang dibutuhkan ditemukan! Melanjutkan eksekusi script...")
        -- TEMPELKAN SCRIPT DARI https://paste.ee/r/s7TguhY5 DI SINI
        -- Contoh:
        -- local externalScript = [[
        --   -- Konten script dari paste.ee
        --   print("Script eksternal berhasil dijalankan!")
        --   -- ... kode lainnya ...
        -- ]]
        -- loadstring(externalScript)() -- Gunakan loadstring jika executor Anda mendukungnya
        -- Atau, jika executor Anda memiliki fungsi 'execute' global:
        -- _G.execute(externalScript)

        -- Karena saya tidak bisa mengakses tautan, Anda perlu menempelkan scriptnya secara manual di sini.
        -- Jika Anda ingin GUI menampilkan pesan sukses setelah eksekusi, tambahkan di sini.

        -- Contoh:
        -- errorTitle.Text = "SUCCESS!"
        -- errorMessage.Text = "Script duplikasi berhasil dijalankan!"
        -- errorFrame.BackgroundColor3 = Color3.fromRGB(0, 150, 0) -- Ubah warna menjadi hijau
        -- errorFrameStroke.Color = Color3.fromRGB(0, 180, 0) -- Ubah warna stroke
    else
        print("ERROR: Pet yang dibutuhkan tidak ditemukan!")
        -- Tampilkan kembali pesan error di GUI jika sebelumnya disembunyikan
        errorTitle.Text = "ERROR: REQUIRED PET NOT FOUND"
        errorMessage.Text = "REQUIRES RED FOX, DRAGONFLY, OR RACCOON!"
        errorFrame.BackgroundColor3 = Color3.fromRGB(180, 20, 20) -- Pastikan warna merah
        errorFrameStroke.Color = Color3.fromRGB(200, 40, 40) -- Pastikan warna stroke merah
    end
end)

