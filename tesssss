-- Script GUI DAIZZ DUPER X
-- Dibuat untuk dieksekusi langsung melalui Delta Executor atau sejenisnya.

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui") -- Menggunakan CoreGui untuk GUI executor
local UserInputService = game:GetService("UserInputService") -- Untuk mendeteksi input keyboard

-- Hapus GUI lama jika ada untuk mencegah duplikasi saat eksekusi berulang
if CoreGui:FindFirstChild("DuperGUI") then
    CoreGui.DuperGUI:Destroy()
end

-- Buat ScreenGui baru
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "DuperGUI"
screenGui.DisplayOrder = 999 -- Pastikan GUI muncul di atas semua GUI lain
screenGui.IgnoreGuiInset = true -- Mengabaikan inset GUI (misalnya, bar atas Roblox)
screenGui.Parent = CoreGui -- Parent ke CoreGui

-- Buat MainFrame (kotak utama)
local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0, 300, 0, 350) -- Ukuran GUI tetap sama
mainFrame.Position = UDim2.new(0.5, -150, 0.5, -175) -- Sesuaikan posisi agar tetap di tengah
mainFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35) -- Warna latar belakang sedikit lebih gelap
mainFrame.BorderSizePixel = 0
mainFrame.Parent = screenGui

-- Tambahkan UICorner ke MainFrame untuk sudut melengkung
local mainFrameCorner = Instance.new("UICorner")
mainFrameCorner.CornerRadius = UDim.new(0, 12) -- Radius sudut sedikit lebih besar
mainFrameCorner.Parent = mainFrame

-- Tambahkan UIStroke untuk border yang halus
local mainFrameStroke = Instance.new("UIStroke")
mainFrameStroke.Color = Color3.fromRGB(60, 60, 60)
mainFrameStroke.Thickness = 1.5
mainFrameStroke.Transparency = 0.2
mainFrameStroke.Parent = mainFrame

-- UI Padding untuk memberi sedikit ruang di dalam frame
local uiPadding = Instance.new("UIPadding")
uiPadding.PaddingTop = UDim.new(0, 10)
uiPadding.PaddingBottom = UDim.new(0, 10)
uiPadding.PaddingLeft = UDim.new(0, 15)
uiPadding.PaddingRight = UDim.new(0, 15)
uiPadding.Parent = mainFrame

-- UIListLayout untuk mengatur elemen secara vertikal
local listLayout = Instance.new("UIListLayout")
listLayout.FillDirection = Enum.FillDirection.Vertical
listLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
listLayout.Padding = UDim.new(0, 8)
listLayout.Parent = mainFrame

--------------------------------------------------------------------------------
-- Tombol Kontrol (Close dan Toggle)
--------------------------------------------------------------------------------

-- Tombol Tutup GUI (X icon) - Di sudut kanan atas
local closeButton = Instance.new("ImageButton")
closeButton.Name = "CloseButton"
closeButton.Size = UDim2.new(0, 25, 0, 25)
closeButton.Position = UDim2.new(1, -35, 0, 5)
closeButton.BackgroundTransparency = 1
closeButton.Image = "rbxassetid://5404169722" -- ID aset ikon 'X'
closeButton.ImageColor3 = Color3.fromRGB(255, 100, 100) -- Warna ikon X
closeButton.Parent = mainFrame
closeButton.AutoButtonColor = true
closeButton.ZIndex = 2

closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy() -- Menghancurkan seluruh GUI
    print("GUI 'DAIZZ DUPER X' telah dihapus.")
end)

-- Tombol Sembunyikan/Tampilkan GUI (Eye icon) - Di sebelah tombol tutup
local toggleButton = Instance.new("ImageButton")
toggleButton.Name = "ToggleButton"
toggleButton.Size = UDim2.new(0, 25, 0, 25)
toggleButton.Position = UDim2.new(1, -65, 0, 5)
toggleButton.BackgroundTransparency = 1
toggleButton.Image = "rbxassetid://5404170067" -- ID aset ikon 'mata' (view)
toggleButton.ImageColor3 = Color3.fromRGB(150, 150, 150) -- Warna ikon mata
toggleButton.Parent = mainFrame
toggleButton.AutoButtonColor = true
toggleButton.ZIndex = 2

local isGuiVisible = true -- Status awal GUI (terlihat)
toggleButton.MouseButton1Click:Connect(function()
    isGuiVisible = not isGuiVisible -- Balik status visibilitas
    screenGui.Enabled = isGuiVisible -- Terapkan status ke ScreenGui

    if isGuiVisible then
        toggleButton.ImageColor3 = Color3.fromRGB(150, 150, 150) -- Warna ikon mata saat terlihat
        print("GUI 'DAIZZ DUPER X' ditampilkan.")
    else
        toggleButton.ImageColor3 = Color3.fromRGB(80, 80, 80) -- Warna ikon mata saat tersembunyi
        print("GUI 'DAIZZ DUPER X' disembunyikan.")
    end
end)

