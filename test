print("GUI 'DAIZZ DUPER X' telah dimuat!")

--------------------------------------------------------------------------------
-- Sistem Sembunyikan/Tampilkan GUI
--------------------------------------------------------------------------------
local isGuiVisible = true -- Status awal GUI (terlihat)
local toggleKey = Enum.KeyCode.Insert -- Tombol untuk menyembunyikan/menampilkan GUI (bisa diganti)

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    -- Pastikan input bukan dari game (misal: mengetik di chat)
    if not gameProcessedEvent and input.KeyCode == toggleKey then
        isGuiVisible = not isGuiVisible -- Balik status visibilitas
        screenGui.Enabled = isGuiVisible -- Terapkan status ke ScreenGui

        if isGuiVisible then
            print("GUI 'DAIZZ DUPER X' ditampilkan.")
        else
            print("GUI 'DAIZZ DUPER X' disembunyikan. Tekan '" .. toggleKey.Name .. "' lagi untuk menampilkan.")
        end
    end
end)

print("Tekan tombol '" .. toggleKey.Name .. "' untuk menyembunyikan/menampilkan GUI.")
