-- Script GUI DAIZZ DUPER X
-- Dibuat untuk dieksekusi langsung melalui Delta Executor atau sejenisnya.

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local CoreGui = game:GetService("CoreGui") -- Menggunakan CoreGui untuk GUI executor
local UserInputService = game:GetService("UserInputService") -- Untuk mendeteksi input keyboard

-- Hapus GUI lama jika ada untuk mencegah duplikasi saat eksekusi berulang
if CoreGui:FindFirstChild("DuperGUI") then
    CoreGui.DuperGUI:Destroy()
end

-- Buat ScreenGui baru
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "DuperGUI"
screenGui.DisplayOrder = 999 -- Pastikan GUI muncul di atas semua GUI lain
screenGui.IgnoreGuiInset = true -- Mengabaikan inset GUI (misalnya, bar atas Roblox)
screenGui.Parent = CoreGui -- Parent ke CoreGui

-- Buat MainFrame (kotak utama)
local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0, 350, 0, 400) -- Ukuran GUI diperkecil
mainFrame.Position = UDim2.new(0.5, -175, 0.5, -200) -- Sesuaikan posisi agar tetap di tengah
mainFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
mainFrame.BorderSizePixel = 0
mainFrame.Parent = screenGui

-- Tambahkan UICorner ke MainFrame untuk sudut melengkung
local mainFrameCorner = Instance.new("UICorner")
mainFrameCorner.CornerRadius = UDim.new(0, 10) -- Sesuaikan radius
mainFrameCorner.Parent = mainFrame

-- UI Padding untuk memberi sedikit ruang di dalam frame
local uiPadding = Instance.new("UIPadding")
uiPadding.PaddingTop = UDim.new(0, 15)
uiPadding.PaddingBottom = UDim.new(0, 15)
uiPadding.PaddingLeft = UDim.new(0, 20)
uiPadding.PaddingRight = UDim.new(0, 20)
uiPadding.Parent = mainFrame

-- UIListLayout untuk mengatur elemen secara vertikal
local listLayout = Instance.new("UIListLayout")
listLayout.FillDirection = Enum.FillDirection.Vertical
listLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
listLayout.Padding = UDim.new(0, 10) -- Jarak antar elemen
listLayout.Parent = mainFrame


--------------------------------------------------------------------------------
-- Header Section
--------------------------------------------------------------------------------
local headerFrame = Instance.new("Frame")
headerFrame.Name = "HeaderFrame"
headerFrame.Size = UDim2.new(1, 0, 0, 30) -- Lebar penuh, tinggi 30
headerFrame.BackgroundTransparency = 1
headerFrame.Parent = mainFrame

local headerListLayout = Instance.new("UIListLayout")
headerListLayout.FillDirection = Enum.FillDirection.Horizontal
headerListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
headerListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
headerListLayout.Padding = UDim.new(0, 5)
headerListLayout.Parent = headerFrame

-- Icon Petir (Lightning)
local lightningIcon = Instance.new("ImageLabel")
lightningIcon.Name = "LightningIcon"
lightningIcon.Size = UDim2.new(0, 25, 0, 25)
lightningIcon.BackgroundTransparency = 1
lightningIcon.Image = "rbxassetid://6034177579" -- Ganti dengan ID ikon petir yang sesuai (contoh: warning icon)
lightningIcon.Parent = headerFrame

-- Judul "DAIZZ DUPER X"
local titleText = Instance.new("TextLabel")
titleText.Name = "TitleText"
titleText.Size = UDim2.new(0, 160, 0, 30) -- Sesuaikan ukuran
titleText.BackgroundTransparency = 1
titleText.Font = Enum.Font.SourceSansBold -- Font tebal
titleText.TextColor3 = Color3.fromRGB(255, 255, 255)
titleText.TextSize = 24
titleText.Text = "DAIZZ DUPER X"
titleText.TextXAlignment = Enum.TextXAlignment.Left
titleText.Parent = headerFrame

-- Versi "(v3.2.1)"
local versionText = Instance.new("TextLabel")
versionText.Name = "VersionText"
versionText.Size = UDim2.new(0, 80, 0, 30) -- Sesuaikan ukuran
versionText.BackgroundTransparency = 1
versionText.Font = Enum.Font.SourceSansLight -- Font lebih tipis
versionText.TextColor3 = Color3.fromRGB(200, 200, 200)
versionText.TextSize = 18
versionText.Text = "(v3.2.1)"
versionText.TextXAlignment = Enum.TextXAlignment.Left
versionText.Parent = headerFrame

--------------------------------------------------------------------------------
-- Error Section
--------------------------------------------------------------------------------
local errorFrame = Instance.new("Frame")
errorFrame.Name = "ErrorFrame"
errorFrame.Size = UDim2.new(1, 0, 0, 80) -- Tinggi diperkecil
