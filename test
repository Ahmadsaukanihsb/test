--------------------------------------------------------------------------------
-- Try Again Button
--------------------------------------------------------------------------------
local tryAgainButton = Instance.new("TextButton")
tryAgainButton.Name = "TryAgainButton"
tryAgainButton.Size = UDim2.new(0.8, 0, 0, 40) -- Tinggi tombol diperkecil
tryAgainButton.BackgroundColor3 = Color3.fromRGB(200, 30, 30) -- Warna merah tombol
tryAgainButton.BorderSizePixel = 0
tryAgainButton.Font = Enum.Font.SourceSansBold
tryAgainButton.TextColor3 = Color3.fromRGB(255, 255, 255)
tryAgainButton.TextSize = 20 -- Ukuran font diperkecil
tryAgainButton.Text = "TRY AGAIN"
tryAgainButton.Parent = mainFrame

-- Tambahkan UICorner ke tombol
local buttonCorner = Instance.new("UICorner")
buttonCorner.CornerRadius = UDim.new(0, 8) -- Sesuaikan radius
buttonCorner.Parent = tryAgainButton

-- Contoh event saat tombol diklik (bisa diganti sesuai kebutuhanmu)
tryAgainButton.MouseButton1Click:Connect(function()
    print("Tombol 'TRY AGAIN' diklik!")
    -- Di sini kamu bisa menambahkan logika ketika tombol diklik
    -- Misalnya, mencoba lagi proses dupe, dll.
end)

--------------------------------------------------------------------------------
-- Footer (Discord Link)
--------------------------------------------------------------------------------
local footerFrame = Instance.new("Frame")
footerFrame.Name = "FooterFrame"
footerFrame.Size = UDim2.new(1, 0, 0, 18) -- Tinggi footer diperkecil
footerFrame.BackgroundTransparency = 1
footerFrame.Parent = mainFrame

local footerText = Instance.new("TextLabel")
footerText.Name = "FooterText"
