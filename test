errorListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
errorListLayout.Padding = UDim.new(0, 5)
errorListLayout.Parent = errorFrame

-- Icon X Merah
local errorIcon = Instance.new("ImageLabel")
